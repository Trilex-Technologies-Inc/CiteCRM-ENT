#content{
	padding:5px;
}	

.toolbar {
	background-image: url(http://www.citecrm.com/images/theme/swsh.gif);
	text-decoration: none;
	height:20px;
}

ul.subpanelTablist{
	padding: 3px 0;
	margin: 0;
	font: 12px Tahoma,Verdana;
	font-weight: bold;
}
 
ul.subpanelTablist li{
	list-style: none;
	margin: 0;
	display: inline;
}

ul.subpanelTablist li a{
	background-repeat: repeat-x;
	background-position: top;
	padding: 3px 8px;
	margin: 0px;
	border-top: 1px solid #1c679f;
	border-right: 1px solid #1c679f;
	border-bottom: none;
	text-decoration: none;
}

ul.subpanelTablist li a:link { text-decoration: none; }
ul.subpanelTablist li a:visited { text-decoration: none; }
ul.subpanelTablist li a:active { text-decoration: none; }

ul.subpanelTablist li a:hover{
	border-top: 1px solid #1c679f;
	background-color: #6699FF;
	color:#FFFFFF;
}

ul.subpanelTablist li a.current, ul.subpanelTablist li a.current:hover{	
	border-left: 1px solid #1c679f;
	font-weight: bold;
	background-color: #6699FF;
	color:#FFFFFF;
}

#spaceBox{
	background-color: #6699FF;	
	height:8px;
	border-bottom: 1px solid #1c679f;
	border-left: 1px solid #1c679f;
	border-right:1px solid #1c679f;
}

#contentBox {
	
}

#callTimer {
	color: red;
	text-decoration:blink;
}

#dhtmltooltip{
position: absolute;
width: 250px;
border: 1px solid black;
padding: 2px;
background-color: #efefef;
visibility: hidden;
z-index: 100;

}

