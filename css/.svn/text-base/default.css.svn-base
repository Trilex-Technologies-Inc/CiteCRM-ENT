/* Form Fields */



FORM {
    display: inline;
    margin:0px;
  }


TD.fieldKey {
    font-family:Tahoma, Verdana, Arial, sans-serif;
    font-size: 11px;
    font-weight: bold;
  }


TD.fieldValue {
    font-family:Tahoma, Verdana, Arial, sans-serif;
    font-size: 11px;
  }


CHECKBOX, INPUT, RADIO, SELECT {
    font-family:Tahoma, Verdana, Arial, sans-serif;
    font-size: 11px;
  }


TEXTAREA {
    width: 100%;
    font-family:Tahoma, Verdana, Arial, sans-serif;
    font-size: 11px;
  }

.box {
	background: #f1f9fe;
	border-top-width: 1px;
	border-top-color: #7b9ebd;
	border-top-style: solid;
	border-bottom-width: 1px;
	border-bottom-color: #7b9ebd;
	border-bottom-style: solid;
}


TABLE.formArea {
    background: #f1f9fe;
    border-color: #7b9ebd;
    border-style: solid;
    border-width: 1px;
  }


TD.formAreaTitle {
    font-family: Tahoma,Tahoma, Verdana, Arial, sans-serif;
    font-size: 11px;
    font-weight: bold;
  }


.inputRequirement {
