<?xml version="1.0"?>
<schema version="0.2">
	<table name="operating_system">
		<field name="operating_system_id" type="I" size="20">
			<KEY/>
			<AUTOINCREMENT/>
		</field>
		<field name="operating_system_manufacture_id" type="I" size="20">
			<NOTNULL/>
			<DEFAULT value="0"/>
		</field>
		<field name="operating_system_name" type="C" size="100">
			<NOTNULL/>
		</field>
		<field name="operating_system_active" type="I" size="4">
			<NOTNULL/>
			<DEFAULT value="0"/>
		</field>
		<field name="last_modified" type="T">
			<DEFAULT value="CURRENT_TIMESTAMP"/>
		</field>
		<index name="operating_system_manufacture_id">
			<col>operating_system_manufacture_id</col>
		</index>
	</table>
</schema>