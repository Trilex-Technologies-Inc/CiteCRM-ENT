<?xml version="1.0"?>
<schema version="0.2">
	<table name="file">
		<field name="file_id" type="I" size="20">
			<KEY/>
			<AUTOINCREMENT/>
		</field>
		<field name="file_type" type="C" size="20">
			<NOTNULL/>
		</field>
		<field name="file_type_id" type="I" size="20">
			<NOTNULL/>
		</field>
		<field name="file_size" type="N" size="20"></field>
		<field name="file_name" type="C" size="128">
			<NOTNULL/>
		</field>
		<field name="file_ext" type="C" size="10"></field>
		<field name="file_create_date" type="I" size="20"></field>
		<field name="file_upload_by" type="I" size="20"></field>
		<field name="file_active" type="I" size="3"></field>
		<field name="file_mime_type" type="C" size="64"></field>
		<field name="file_path" type="C" size="128"></field>
		<field name="last_modified" type="T">
			<DEFAULT value="CURRENT_TIMESTAMP"/>
		</field>
		<index name="file_type_id">
			<col>file_type_id</col>
		</index>
		<index name="file_upload_by">
			<col>file_upload_by</col>
		</index>
		<index name="file_active">
			<col>file_active</col>
		</index>
	</table>
</schema>