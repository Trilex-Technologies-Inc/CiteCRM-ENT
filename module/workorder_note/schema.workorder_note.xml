<?xml version="1.0"?>
<schema version="0.2">
	<table name="workorder_note">
		<field name="workorder_note_id" type="I" size="20">
			<KEY/>
			<AUTOINCREMENT/>
		</field>
		<field name="workorder_id" type="I" size="20">
			<NOTNULL/>
			<DEFAULT value="0"/>
		</field>
		<field name="workorder_note_create_by" type="I" size="20">
			<NOTNULL/>
			<DEFAULT value="0"/>
		</field>
		<field name="workorder_note_text" type="X">
			<NOTNULL/>
		</field>
		<field name="last_modified" type="T">
			<DEFAULT value="CURRENT_TIMESTAMP"/>
		</field>
		<index name="workorder_id">
			<col>workorder_id</col>
		</index>
		<index name="workorder_note_create_by">
			<col>workorder_note_create_by</col>
		</index>
	</table>
</schema>