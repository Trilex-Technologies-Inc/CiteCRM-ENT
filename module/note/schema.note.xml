<?xml version="1.0"?>
<schema version="0.2">
	<table name="note">
		<field name="note_id" type="I" size="20">
			<KEY/>
			<AUTOINCREMENT/>
		</field>
		<field name="note_text" type="X">
			<NOTNULL/>
		</field>
		<field name="note_type" type="C" size="10"></field>
		<field name="note_type_id" type="I" size="20"></field>
		<field name="note_enter_by" type="I" size="20"></field>
		<field name="note_create_date" type="I" size="20"></field>
		<field name="note_active" type="I" size="3"></field>
		<field name="last_modified" type="T">
			<DEFAULT value="CURRENT_TIMESTAMP"/>
		</field>
	</table>
</schema>