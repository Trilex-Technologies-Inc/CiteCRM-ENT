<?xml version="1.0"?>
<schema version="0.2">
	<table name="vendor_contact">
		<field name="vendor_contact_id" type="I" size="20">
			<KEY/>
			<AUTOINCREMENT/>
		</field>
		<field name="vendor_id" type="I" size="20"></field>
		<field name="vendor_contact_type" type="I" size="20"></field>
		<field name="vendor_contact_value" type="C" size="200"></field>
		<field name="last_modified" type="T">
			<DEFAULT value="CURRENT_TIMESTAMP"/>
		</field>
		<index name="vendor_id">
			<col>vendor_id</col>
		</index>
	</table>
</schema>