<?xml version="1.0"?>
<schema version="0.2">
	<table name="company_contact">
		<field name="company_contact_id" type="I" size="20">
			<KEY/>
			<AUTOINCREMENT/>
		</field>
		<field name="company_id" type="I" size="20"></field>
		<field name="company_contact_type" type="C" size="14"></field>
		<field name="company_contact_value" type="C" size="100">
			<NOTNULL/>
		</field>
		<field name="last_modified" type="T">
			<DEFAULT value="CURRENT_TIMESTAMP"/>
		</field>
		<index name="company_id">
			<col>company_id</col>
		</index>
	</table>
</schema>