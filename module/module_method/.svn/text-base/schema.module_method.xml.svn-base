<?xml version="1.0"?>
<schema version="0.2">
	<table name="module_method">
		<field name="module_method_id" type="I" size="20">
			<KEY/>
			<AUTOINCREMENT/>
		</field>
		<field name="module_id" type="I" size="20"></field>
		<field name="module_method_name" type="C" size="100">
			<NOTNULL/>
		</field>
		<field name="module_method_translate" type="C" size="100">
			<NOTNULL/>
		</field>
		<field name="module_method_admin_menu" type="I" size="4"></field>
		<field name="module_method_user_menu" type="I" size="4"></field>
		<field name="module_method_public_menu" type="I" size="4"></field>
		<field name="last_modified" type="T">
			<DEFAULT value="CURRENT_TIMESTAMP"/>
		</field>
		<index name="module_id">
			<col>module_id</col>
		</index>
	</table>
</schema>