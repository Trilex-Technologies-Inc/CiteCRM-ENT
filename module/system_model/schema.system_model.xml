<?xml version="1.0"?>
<schema version="0.2">
	<table name="system_model">
		<field name="system_model_id" type="I" size="20">
			<KEY/>
			<AUTOINCREMENT/>
		</field>
		<field name="system_manufacture_id" type="I" size="20">
			<NOTNULL/>
			<DEFAULT value="0"/>
		</field>
		<field name="system_model_name" type="C" size="100">
			<NOTNULL/>
		</field>
		<field name="system_full_name" type="C" size="100">
			<NOTNULL/>
		</field>
		<field name="last_modified" type="T">
			<DEFAULT value="CURRENT_TIMESTAMP"/>
		</field>
		<index name="system_manufacture_id">
			<col>system_manufacture_id</col>
		</index>
	</table>
</schema>