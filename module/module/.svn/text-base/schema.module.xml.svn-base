<?xml version="1.0"?>
<schema version="0.2">
	<table name="module">
		<field name="module_id" type="I" size="20">
			<KEY/>
			<AUTOINCREMENT/>
		</field>
		<field name="module_name" type="C" size="100">
			<NOTNULL/>
		</field>
		<field name="module_translate_name" type="C" size="100">
			<NOTNULL/>
		</field>
		<field name="module_admin_menu" type="I" size="4">
			<NOTNULL/>
			<DEFAULT value="0"/>
		</field>
		<field name="module_user_menu" type="I" size="4">
			<NOTNULL/>
			<DEFAULT value="0"/>
		</field>
		<field name="module_public_menu" type="I" size="4">
			<NOTNULL/>
			<DEFAULT value="0"/>
		</field>
		<field name="last_modified" type="T">
			<DEFAULT value="CURRENT_TIMESTAMP"/>
		</field>
	</table>
</schema>