<?xml version="1.0"?>
<schema version="0.2">
	<table name="billing_rates">
		<field name="billing_rates_id" type="I" size="20">
			<KEY/>
			<AUTOINCREMENT/>
		</field>
		<field name="billing_rate_text" type="C" size="100">
			<NOTNULL/>
		</field>
		<field name="billing_rate_amount" type="N" size="10"></field>
		<field name="billing_rate_active" type="I" size="4">
			<DEFAULT value="1"/>
		</field>
		<field name="last_modified" type="T">
			<DEFAULT value="CURRENT_TIMESTAMP"/>
		</field>
	</table>
</schema>