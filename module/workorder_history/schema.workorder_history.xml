<?xml version="1.0"?>
<schema version="0.2">
	<table name="workorder_history">
		<field name="workorder_history_id" type="I" size="20">
			<KEY/>
			<AUTOINCREMENT/>
		</field>
		<field name="workorder_id" type="I" size="20">
			<NOTNULL/>
			<DEFAULT value="0"/>
		</field>
		<field name="user_id" type="I" size="20">
			<NOTNULL/>
			<DEFAULT value="0"/>
		</field>
		<field name="workorder_history_text" type="C">
			<NOTNULL/>
		</field>
		<field name="workorder_history_create_date" type="I" size="20">
			<NOTNULL/>
			<DEFAULT value="0"/>
		</field>
		<field name="last_modified" type="T">
			<DEFAULT value="CURRENT_TIMESTAMP"/>
		</field>
		<index name="workorder_id">
			<col>workorder_id</col>
		</index>
		<index name="user_id">
			<col>user_id</col>
		</index>
	</table>
</schema>