<?xml version="1.0"?>
<schema version="0.2">
	<table name="workorder_status">
		<field name="workorder_status_id" type="I" size="20">
			<KEY/>
			<AUTOINCREMENT/>
		</field>
		<field name="workorder_status_text" type="C" size="100">
			<NOTNULL/>
		</field>
		<field name="workorder_status_active" type="I" size="4">
			<DEFAULT value="1"/>
		</field>
		<field name="last_modified" type="T">
			<DEFAULT value="CURRENT_TIMESTAMP"/>
		</field>
	</table>
</schema>