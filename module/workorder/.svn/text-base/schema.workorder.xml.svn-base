<?xml version="1.0"?>
<schema version="0.2">
	<table name="workorder">
		<field name="workorder_id" type="I" size="20">
			<KEY/>
			<AUTOINCREMENT/>
		</field>
		<field name="workorder_open_date" type="I" size="20">
			<NOTNULL/>
			<DEFAULT value="0"/>
		</field>
		<field name="workorder_status" type="I" size="20">
			<NOTNULL/>
			<DEFAULT value="0"/>
		</field>
		<field name="workorder_active" type="I" size="4">
			<NOTNULL/>
			<DEFAULT value="0"/>
		</field>
		<field name="workorder_create_by" type="I" size="20">
			<NOTNULL/>
			<DEFAULT value="0"/>
		</field>
		<field name="workorder_assigned_to" type="I" size="20"></field>
		<field name="workorder_scope" type="C" size="100">
			<NOTNULL/>
		</field>
		<field name="workorder_desription" type="X"></field>
		<field name="workorder_close_date" type="I" size="20">
			<NOTNULL/>
			<DEFAULT value="0"/>
		</field>
		<field name="workorder_close_by" type="I" size="20">
			<NOTNULL/>
			<DEFAULT value="0"/>
		</field>
		<field name="workorder_resolution" type="X"></field>
		<field name="last_modified" type="T">
			<DEFAULT value="CURRENT_TIMESTAMP"/>
		</field>
		<index name="workorder_create_by">
			<col>workorder_create_by</col>
		</index>
		<index name="workorder_assigned_to">
			<col>workorder_assigned_to</col>
		</index>
		<index name="workorder_close_by">
			<col>workorder_close_by</col>
		</index>
	</table>
</schema>