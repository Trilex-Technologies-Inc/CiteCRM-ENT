<?xml version="1.0"?>
<schema version="0.2">
	<table name="company">
		<field name="company_id" type="I" size="20">
			<KEY/>
			<AUTOINCREMENT/>
		</field>
		<field name="company_name" type="C" size="60">
			<NOTNULL/>
		</field>
		<field name="company_website" type="C" size="100"></field>
		<field name="company_create_date" type="I" size="20">
			<NOTNULL/>
			<DEFAULT value="0"/>
		</field>
		<field name="company_active" type="I" size="4">
			<DEFAULT value="1"/>
		</field>
		<field name="last_modified" type="T">
			<DEFAULT value="CURRENT_TIMESTAMP"/>
		</field>
	</table>
</schema>