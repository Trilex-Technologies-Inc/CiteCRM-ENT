<?xml version="1.0"?>
<schema version="0.2">
	<table name="product_to_category">
		<field name="product_to_category_id" type="I" size="20">
			<KEY/>
			<AUTOINCREMENT/>
		</field>
		<field name="product_id" type="I" size="11"></field>
		<field name="category_id" type="I" size="11"></field>
		<field name="last_modified" type="T">
			<DEFAULT value="CURRENT_TIMESTAMP"/>
		</field>
		<index name="product_id">
			<col>product_id</col>
		</index>
		<index name="category_id">
			<col>category_id</col>
		</index>
	</table>
</schema>