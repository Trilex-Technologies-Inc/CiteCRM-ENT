<?xml version="1.0"?>
<schema version="0.2">
	<table name="contract_type">
		<field name="contract_type_id" type="I" size="20">
			<KEY/>
			<AUTOINCREMENT/>
		</field>
		<field name="contract_type_name" type="C" size="128">
			<NOTNULL/>
		</field>
		<field name="contract_type_text" type="X"></field>
		<field name="contract_type_rate" type="N" size="10"></field>
		<field name="contract_type_term" type="C" size="128"></field>
		<field name="contract_type_active" type="I" size="3"></field>
		<field name="last_modified" type="T">
			<DEFAULT value="CURRENT_TIMESTAMP"/>
		</field>
	</table>
</schema>