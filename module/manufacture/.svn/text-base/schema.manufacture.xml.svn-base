<?xml version="1.0"?>
<schema version="0.2">
	<table name="manufacture">
		<field name="manufacture_id" type="I" size="20">
			<KEY/>
			<AUTOINCREMENT/>
		</field>
		<field name="manufacture_name" type="C" size="64"></field>
		<field name="manufacture_image" type="C" size="64"></field>
		<field name="last_modified" type="T">
			<DEFAULT value="CURRENT_TIMESTAMP"/>
		</field>
	</table>
</schema>