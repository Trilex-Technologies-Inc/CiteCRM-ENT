<?xml version="1.0"?>
<schema version="0.2">
	<table name="payment_option">
		<field name="payment_option_id" type="I" size="20">
			<KEY/>
			<AUTOINCREMENT/>
		</field>
		<field name="payment_option_text" type="C" size="64">
			<NOTNULL/>
		</field>
		<field name="payment_option_active" type="I" size="3"></field>
		<field name="payment_option_order" type="I" size="8"></field>
		<field name="last_modified" type="T">
			<DEFAULT value="CURRENT_TIMESTAMP"/>
		</field>
	</table>
</schema>