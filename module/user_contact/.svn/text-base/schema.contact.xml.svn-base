<?xml version="1.0"?>
<schema version="0.2">
	<table name="contact">
		<field name="contact_id" type="I" size="20">
			<KEY/>
			<AUTOINCREMENT/>
		</field>
		<field name="user_id" type="I" size="20">
			<NOTNULL/>
			<DEFAULT value="0"/>
		</field>
		<field name="account_id" type="I" size="20">
			<NOTNULL/>
			<DEFAULT value="0"/>
		</field>
		<field name="contact_type" type="C" size="14">
			<NOTNULL/>
			<DEFAULT value="Personal Phone"/>
		</field>
		<field name="contact_value" type="C" size="100">
			<NOTNULL/>
		</field>
		<field name="contact_create_date" type="I" size="20">
			<NOTNULL/>
			<DEFAULT value="0"/>
		</field>
		<field name="last_modified" type="T">
			<DEFAULT value="CURRENT_TIMESTAMP"/>
		</field>
		<index name="user_id">
			<col>user_id</col>
			<col>account_id</col>
		</index>
	</table>
</schema>