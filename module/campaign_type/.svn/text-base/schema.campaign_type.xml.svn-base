<?xml version="1.0"?>
<schema version="0.2">
	<table name="campaign_type">
		<field name="campaign_type_id" type="I" size="20">
			<KEY/>
			<AUTOINCREMENT/>
		</field>
		<field name="campaign_type_text" type="C" size="128">
			<NOTNULL/>
		</field>
		<field name="campaign_type_active" type="I" size="3"></field>
		<field name="last_modified" type="T">
			<DEFAULT value="CURRENT_TIMESTAMP"/>
		</field>
	</table>
</schema>