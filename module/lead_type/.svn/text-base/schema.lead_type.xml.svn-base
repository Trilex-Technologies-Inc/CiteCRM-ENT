<?xml version="1.0"?>
<schema version="0.2">
	<table name="lead_type">
		<field name="lead_type_id" type="I" size="20">
			<KEY/>
			<AUTOINCREMENT/>
		</field>
		<field name="lead_type_text" type="C" size="255"></field>
		<field name="lead_type_active" type="I" size="3"></field>
		<field name="last_modified" type="T">
			<DEFAULT value="CURRENT_TIMESTAMP"/>
		</field>
		<index name="lead_type_active">
			<col>lead_type_active</col>
		</index>
	</table>
</schema>