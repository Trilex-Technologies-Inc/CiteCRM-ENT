<?xml version="1.0"?>
<schema version="0.2">
	<table name="workorder_time">
		<field name="workorder_time_id" type="I" size="20">
			<KEY/>
			<AUTOINCREMENT/>
		</field>
		<field name="workorder_id" type="I" size="20"></field>
		<field name="user_id" type="I" size="20"></field>
		<field name="workorder_time_start" type="I" size="20"></field>
		<field name="workorder_time_end" type="I" size="20"></field>
		<field name="last_modified" type="T">
			<DEFAULT value="CURRENT_TIMESTAMP"/>
		</field>
		<index name="workorder_id">
			<col>workorder_id</col>
		</index>
		<index name="user_id">
			<col>user_id</col>
		</index>
	</table>
</schema>