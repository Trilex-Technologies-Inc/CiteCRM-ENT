<?xml version="1.0"?>
<schema version="0.2">
	<table name="vendor">
		<field name="vendor_id" type="I" size="20">
			<KEY/>
			<AUTOINCREMENT/>
		</field>
		<field name="vendor_name" type="C" size="60"></field>
		<field name="vendor_website" type="C" size="100"></field>
		<field name="vendor_create_date" type="I" size="20"></field>
		<field name="vendor_active" type="I" size="4"></field>
		<field name="last_modified" type="T">
			<DEFAULT value="CURRENT_TIMESTAMP"/>
		</field>
	</table>
</schema>