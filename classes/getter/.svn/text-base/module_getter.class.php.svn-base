		return $this->fields['module_name'];
	}
	
	function get_module_translate_name() {
		return $this->fields['module_translate_name'];
	} 
	
	function get_module_admin_menu() {
		return $this->fields['module_admin_menu'];
	}
	
	function get_module_user_menu() {
		return $this->fields['module_user_menu'];
	} 
	
	function get_module_public_menu() {
		return $this->fields['module_public_menu'];
	} 
	
	function get_last_modified() {
