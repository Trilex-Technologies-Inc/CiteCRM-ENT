	}
	
	function get_module_method_translate() {
		return $this->fields['module_method_translate'];
	} 
	
	function get_module_method_admin_menu() {
		return $this->fields['module_method_admin_menu'];
	} 
	
	function get_module_method_user_menu() {
		return $this->fields['module_method_user_menu'];
	} 
	
	function get_module_method_public_menu() {
		return $this->fields['module_method_public_menu'];
	} 

