<!-- add_billing_rates_frm -->
<br>
<table cellpadding="0" cellspacing="0" width="400">
	<tr>
		<td><span class="greetUser">Add New Billing Rate</td>
	</tr>
</table>

<br>

<form>

<table cellpadding="5" cellspacing="5" class="formArea" width="400">
	<tr>
		<td class="formAreaTitle">Type</td>
		<td class="fieldValue"><input type="text" name="billing_rate_text" value="{$billing_rate_text}" size="" id="billing_rate_text"></td>
	</tr><tr>
		<td class="formAreaTitle">Amount</td>
		<td class="fieldValue">$ <input type="text" name="billing_rate_amount" value="{$billing_rate_amount}" size="10" id="billing_rate_amount"></td>
	</tr><tr>
		<td class="formAreaTitle">Active</td>
		<td class="fieldValue">{html_select_yesno fieldName="billing_rate_active" value="1"}</td>
	</tr><tr>
		<td colspan="4">
			<input type="button" name="submit" value="Save" onclick="save_new_billing_rate()">
			<input type="button" name="cancel" value="Cancel" onclick="cancel_edit()">
		</td>
	</tr>
</table>

</form>
<br>
