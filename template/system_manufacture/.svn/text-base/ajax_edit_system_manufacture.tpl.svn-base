<!--ajax_edit_system_manufacture.tpl-->
<br>

<table cellpadding="0" cellspacing="0" width="400">
	<tr>
		<td><span class="greetUser">Edit Manufacture {$manufactureObj->get_manufacture_name()}</span></td>
	</tr>
</table>

<table cellpadding="5" cellspacing="5" class="formArea" width="400">
	<tr>
		<td class="formAreaTitle">Name</td>
		<td class="fieldValue"><input type="text" name="manufacture_name" id="manufacture_name" value="{$manufactureObj->get_manufacture_name()}"></td>
		<td class="formAreaTitle">Abrv</td>
		<td class="fieldValue"><input type="text" name="manufacture_abrv" id="manufacture_abrv" value="{$manufactureObj->get_manufacture_abrv()}"></td>
	</tr><tr>
		<td colspan="2">
			<input type="hidden" name="system_manufacture_id" id="system_manufacture_id" value="{$manufactureObj->get_system_manufacture_id()}">
			<input type="button" name="save" id="save" value="Save" onclick="save_manufacture()">
			<input type="button" name="delete" id="delete" value="Delete" onclick="delete_system_manufacture()">
			<input type="button" name="cancel" id="cancel" value="Cancel" onclick="cancel()">
		</td>
	</tr>
</table>