<!-- alert_added.tpl -->

{include file="core/header.tpl"}
<ul class="subpanelTablist" id="groupTabs">
	<li id="All_sp_tab"><a  href="" class="current">New Alert Added</a></li>	
</ul>
<table cellpadding="0" cellspacing="0" class="dataTable" width="600">
	<tr>
		<td class="productListing-data" style="background-color: #F0F8FF" >
			<table cellpadding="5" cellspacing="0" style="background-color: #F0F8FF">
				<tr>
					<td class="formAreaTitle">Start Date</td>
					<td class="fieldValue">{$start_dateMonth}-{$start_dateDay}-{$start_dateYear}</td>
					<td class="formAreaTitle">End Date</td>
					<td class="fieldValue">{$start_dateYear}-{$end_dateDay}-{$end_dateYear}</td>
				</tr><tr>
					<td class="formAreaTitle" colspan="4">Subject</td>
				</tr><tr>
					<td class="fieldValue" colspan="4">{$alert_subject}</td>
				</tr><tr>
					<td class="formAreaTitle" colspan="4">Alert Details</td>
				</tr><tr>
					<td class="fieldValue" colspan="4">{$alert_text}</td>
				</tr>
			</table>
		</td>
	</tr>
</table>



{include file="core/footer.tpl"}