<!-- ajax_add_service_frm.tpl -->
<br>
<table cellpadding="5" cellspacing="5" class="formArea" width="600">
    <tr>
        <td class="formAreaTitle" nowrap="true">Service Name</td>
        <td class="fieldValue"><input type="text" name="service_name" id="service_name" size="60"></td>
    </tr><tr>
        <td class="formAreaTitle" nowrap="true">Amount</td>
        <td class="fieldValue"><input type="text" name="service_amount" id="service_amount" size="16"></td>
    </tr><tr>
        <td class="formAreaTitle" nowrap="true">Active</td>
        <td class="fieldValue">{html_select_yesno fieldName="service_active" value="1"}</td>
    </tr>
</table>
<br>
<input type="submit" value="Save" onclick="save_new_service()">
<input type="button" value="Cancel" onclick="cancel()">
