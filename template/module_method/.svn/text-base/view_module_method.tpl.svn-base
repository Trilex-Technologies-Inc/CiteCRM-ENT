<!-- view_module_method -->
{include file="core/header.tpl"}

<table cellpadding="0" cellspacing="0" width="400">
	<tr>
		<td><span class="greetUser">View module_method ID# {$module_method->get_module_method_id()}</span></td>
		<td align="right">
				<a href="{$from}"><img src="images/icons/back_16.gif" alt="back" border="0"></a>
				<a href="index.php?page=module_method:update_module_method&module_method_id={$module_method->get_module_method_id()}"><img src="images/icons/edit_16.gif" border="0" alt="Edit"></a>
				<a href="index.php?page=module_method:delete_module_method&module_method_id={$module_method->get_module_method_id()}"><img src="images/icons/del_16.gif" border="0" alt="Delete"></a>
			</td>
</tr>
</table>

<br><br>

<table cellpadding="5" cellspacing="5" class="formArea" width="400">
	<tr>
		<td class="formAreaTitle">Module ID</td>
		<td class="fieldValue">{$module_method->get_module_id()}</td>
	</tr>
	<tr>
		<td class="formAreaTitle">Method Name</td>
		<td class="fieldValue">{$module_method->get_module_method_name()}</td>
	</tr>
	<tr>
		<td class="formAreaTitle">Translate</td>
		<td class="fieldValue">{$module_method->get_module_method_translate()}</td>
	</tr>
	<tr>
		<td class="formAreaTitle">Admin Menu</td>
		<td class="fieldValue">{$module_method->get_module_method_admin_menu()}</td>
	</tr>
	<tr>
		<td class="formAreaTitle">User Menu</td>
		<td class="fieldValue">{$module_method->get_module_method_user_menu()}</td>
	</tr>
	<tr>
		<td class="formAreaTitle">Public Menu</td>
		<td class="fieldValue">{$module_method->get_module_method_public_menu()}</td>
	</tr>
</table>

<br><br>

